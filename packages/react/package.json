{
  "name": "@material-cards/react",
  "version": "1.0.0",
  "description": "React material card component",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./style.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "dependencies": {
    "@material-cards/core": "^2.0.3"
  },
  "devDependencies": {
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "@types/react": "^18.3.12"
  },
  "publishConfig": {
    "access": "public"
  }
}
