{
  "name": "@material-cards/vue",
  "version": "1.0.0",
  "description": "Vue material card component",
  "license": "MIT",
  "type": "module",
  "main": "./dist/material-cards-vue.umd.cjs",
  "module": "./dist/material-cards-vue.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/material-cards-vue.js",
      "require": "./dist/material-cards-vue.umd.cjs",
      "default": "./dist/material-cards-vue.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "vite build && vue-tsc -p tsconfig.build.json --declaration --emitDeclarationOnly",
    "typecheck": "vue-tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "vue": ">=3.4"
  },
  "dependencies": {
    "@material-cards/core": "^2.0.3"
  },
  "devDependencies": {
    "vite": "^5.4.11",
    "@vitejs/plugin-vue": "^5.1.5",
    "vue-tsc": "^2.1.10",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
